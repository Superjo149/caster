{
  "name": "@amilajack/castv2-client",
  "version": "0.0.1",
  "description": "A Chromecast client based on the new (CASTV2) protocol",
  "author": "amilajack",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "castv2": "~0.1.9",
    "chromecast-scanner": "^0.5.0",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.2",
    "eslint-config-bliss": "^3.3.0",
    "jest": "^24.5.0",
    "synp": "^1.4.0",
    "util": "^0.11.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rm -rf lib && babel src --out-dir lib",
    "lint": "eslint .",
    "test": "jest . --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thibauts/node-castv2-client.git"
  },
  "keywords": [
    "chromecast",
    "chrome",
    "cast",
    "castv2"
  ],
  "jest": {
