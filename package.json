  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 10
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "bliss"
  },
  "renovate": {
    "extends": ["bliss"]
